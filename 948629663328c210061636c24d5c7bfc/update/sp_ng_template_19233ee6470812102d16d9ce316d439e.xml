        <template><![CDATA[<style>
    .ma-3 {
        margin: 3rem;
    }

    .d-flex {
        display: flex;
    }

    .justify-center {
        justify-content: center;
    }

    .mr-1 {
        margin-right: 1rem;
    }

    .mx-1 {
        margin-right: 1rem;
        margin-left: 1rem;
    }

    .invalid-feedback {
        font-size: 11px;
        color: #ff0000;
    }

    #dropdownMenuButton:disabled {
        background: #f6f6f8 !important;
    }
</style>

<div class="ma-3 " ng-if="c.data.evaluation.user.sys_id == c.data.current_user">
    <h2 class="text-center">
        Evaluación Anual
    </h2>
    <form>
        <br><br>

        <div class="row">
            <div class="col-sm-12 col-md-5 "><strong>Objetivos / Iniciativas</strong></div>
            <div class="col-sm-12 col-md-4 "><strong>Entregables</strong></div>
            <div class="col-sm-12 col-md-3 "><strong>Fecha de Entrega</strong></div>
        </div>
        <hr style="margin: 0px 0px;">

        <div class="row">
            <div class="col-sm-12 col-md-5">
                {{ c.formIniciativeYe.iniciativa.no_iniciative}} -
                {{c.formIniciativeYe.iniciativa.iniciative}}
            </div>
            <div class="col-sm-12 col-md-4">
                {{c.formIniciativeYe.iniciativa.deliverable}}
            </div>
            <div class="col-sm-12 col-md-3">
                {{c.formIniciativeYe.iniciativa.work_end | date:'dd-MM-yyyy' }}
            </div>
        </div>
        <br><br>
        <div class=" form-group">
            <label for="caliciacion_ye">${Avance de objetivos}
                <strong>
                    {{c.formIniciativeYe.fields.ye_collaborator_evaluation}}%
                </strong>
            </label>
            <input type="text" class="form-control" id="caliciacion_ye"
                ng-model="c.formIniciativeYe.fields.ye_collaborator_evaluation" ng-model-options="{ debounce: 300 }" />
            <small class="invalid-feedback">{{c.formIniciativeYe.errors.ye_collaborator_evaluation}}</small>
        </div>
        <div class="form-group">
            <label for="comentarios My">${Comentarios}</label>
            <textarea class="form-control" id="comentarios My" rows="3" ng-model-options="{ debounce: 300 }"
                ng-model="c.formIniciativeYe.fields.ye_contributor_comment"></textarea>
            <small class="invalid-feedback">{{c.formIniciativeYe.errors.ye_contributor_comment}}</small>
        </div>
    </form>
    <div class="d-flex justify-center">
        <button class="btn btn-default" ng-click="c.closeEvaluacionYe()">Cerrar</button>
        <button class="mr-1 btn btn-success" ng-click="c.saveEvaluacionYe(true)"
            ng-disabled=" c.formIniciativeYe.errors.ye_collaborator_evaluation || c.formIniciativeYe.errors.ye_contributor_comment">
            Guardar
        </button>
    </div>
</div>


<div class="ma-3 " ng-if="c.data.evaluation.lead.sys_id == c.data.current_user">
    <h2 class="text-center">
        Evaluación anual
    </h2>
    <form>
        <br><br>

        <div class="row">
            <div class="col-sm-12 col-md-5 "><strong>Objetivos / Iniciativas</strong></div>
            <div class="col-sm-12 col-md-4 "><strong>Entregables</strong></div>
            <div class="col-sm-12 col-md-3 "><strong>Fecha de Entrega</strong></div>
        </div>
        <hr style="margin: 0px 0px;">
        <div class="row">
            <div class="col-sm-12 col-md-5">
                {{ c.formIniciativeYe.iniciativa.no_iniciative}} -
                {{c.formIniciativeYe.iniciativa.iniciative}}
            </div>
            <div class="col-sm-12 col-md-4">
                {{c.formIniciativeYe.iniciativa.deliverable}}
            </div>
            <div class="col-sm-12 col-md-3">
                {{c.formIniciativeYe.iniciativa.work_end | date:'dd-MM-yyyy' }}
            </div>
        </div>
        <br><br>

        <h4 class="text-center">${Colaborador}</h4>
        <div class=" form-group">
            <label for="caliciacion_ye">${Evaluacion} <strong>
                    {{c.formIniciativeYe.fields.ye_collaborator_evaluation}}%
                </strong></label>
            <input disabled type="text" class="form-control" id="caliciacion_ye"
                ng-model="c.formIniciativeYe.fields.ye_collaborator_evaluation" />
        </div>
        <div class="form-group">
            <label for="comentarios My">${Comentario Avance de Objetivos}</label>
            <textarea disabled class="form-control" id="comentarios My" rows="3"
                ng-model="c.formIniciativeYe.fields.ye_contributor_comment"></textarea>
        </div>
        <hr>

        <h4 class="text-center">${Lider}</h4>
        <div class=" form-group">
            <label for="caliciacion_ye">${Evaluacion} <strong>
                    {{c.formIniciativeYe.fields.ye_leader_evaluation}}%
                </strong></label>
            <input type="text" class="form-control" id="caliciacion_ye"
                ng-model="c.formIniciativeYe.fields.ye_leader_evaluation" ng-model-options="{ debounce: 300 }" />
            <small class="invalid-feedback">{{c.formIniciativeYe.errors.ye_leader_evaluation}}</small>
        </div>
        <div class="form-group">
            <label for="comentarios My">${Comentario Avance de Objetivos}</label>
            <textarea class="form-control" id="comentarios My" rows="3" ng-model-options="{ debounce: 300 }"
                ng-model="c.formIniciativeYe.fields.ye_leader_comment"></textarea>
            <small class="invalid-feedback">{{c.formIniciativeYe.errors.ye_leader_comment}}</small>
        </div>
    </form>

    <div class="d-flex justify-center">
        <button class="btn btn-default" ng-click="c.closeEvaluacionYe()">Cerrar</button>
        <button class="mr-1 btn btn-success" ng-click="c.saveEvaluacionYe()"
            ng-disabled="c.formIniciativeYe.errors.ye_leader_evaluation || c.formIniciativeYe.errors.ye_leader_comment">Guardar</button>
    </div>



</div>]]></template>
    </sp_ng_template>
</record_update>
